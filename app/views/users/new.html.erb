<h1>Editing User <%= @user.username %></h1>


<%= form_for(@user) do |f| %>
  <p>
    <%= f.label :username %><br/>
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :hashed_password %><br/>
    <%= f.text_field :hashed_password %>
  </p>
  <p>
    <%= f.label :email %><br/>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :role %><br/>
    <%= f.text_field :role %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
