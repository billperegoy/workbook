<h1>My Dashboard</h1>
<table class='table'>
<% @user.owned_books.each do |owned_book| %>
  <tr>
    <td><%= owned_book.book.title %></td>
    <td>
      <div class="progress">
        <div class="progress-bar" style="width: <%= owned_book.percent_complete %>;"><%= owned_book.percent_complete %></div>
      </div>
    </td>
    <td>
      <div class="dropdown">
        <button class="btn dropdown-toggle" type="button" data-toggle="dropdown">
          action
          <span class="caret"></span>
        </button>
        <ul class="dropdown-menu">
          <li><a href="#">Edit Book</a></li>
          <li><a href="#">Print Template</a></li>
          <li><a href="#">Print Work</a></li>
        </ul>
      </div>
    </td>
  </tr>
  <% end %>
</table>
<%= link_to "Add a book", store_items_path %>
