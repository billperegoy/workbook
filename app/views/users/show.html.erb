<h1>My Dashboard</h1>
<table class='table'>
<% @user.owned_books.each do |owned_book| %>
  <tr>
    <td><%= owned_book.book.title %></td>
    <td>
      <div class="progress">
        <% @percent_complete = owned_book.percent_complete %>
        <div class="progress-bar" style="width: <%= @percent_complete %>;"><%= @percent_complete %></div>
      </div>
    </td>
    <td><%= render partial: 'dashboard_dropdown', locals: { owned_book: owned_book } %></td>
  </tr>
  <% end %>
</table>
<%= link_to "Add a book", store_items_path %>
